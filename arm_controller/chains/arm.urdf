<?xml version="1.0"?>
<robot name="MechaArm">
    <link name="world_base_link"></link>
    <link name="base_waist_link"></link>
    <link name="waist_shoulder_link"></link>
    <link name="shoulder_elbow_link"></link>
    <link name="elbow_wrist_roll_link"></link>
    <link name="wrist_roll_wrist_pitch_link"></link>
    <link name="wrist_pitch_claw_link"></link>
    <link name="claw_end_link"></link>

    <joint name="base" type="static">
        <parent link="world_base_link"/>
        <child link="base_waist_link"/>
        <origin xyz="0 0 56.0" rpy="0 0 1.57"/>
                <axis xyz="0 0 1"/>
<!--        <limit lower="-1.57" upper="1.57"/>-->
    </joint>
    <joint name="waist" type="revolute">
        <parent link="base_waist_link"/>
        <child link="waist_shoulder_link"/>
        <origin xyz="0 0 42.93" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-1.57" upper="1.57"/>
    </joint>
    <joint name="shoulder" type="revolute">
        <parent link="waist_shoulder_link"/>
        <child link="shoulder_elbow_link"/>
        <origin xyz="0 0 120.0" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="0.261799" upper="3.14"/>
    </joint>
    <joint name="elbow" type="revolute">
        <parent link="shoulder_elbow_link"/>
        <child link="elbow_wrist_roll_link"/>
        <origin xyz="0 0 118.65" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="0" upper="1.0472"/>
    </joint>
    <joint name="wrist_roll" type="revolute">
        <parent link="elbow_wrist_roll_link"/>
        <child link="wrist_roll_wrist_pitch_link"/>
        <origin xyz="0 0 60.028" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="0" upper="3.14"/>
    </joint>
    <joint name="wrist_pitch" type="revolute">
        <parent link="wrist_roll_wrist_pitch_link"/>
        <child link="wrist_pitch_claw_link"/>
        <origin xyz="0 0 30.17" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
        <limit lower="0" upper="3.14"/>
    </joint>
    <joint name="claw" type="static">
        <parent link="wrist_pitch_claw_link"/>
        <child link="claw_end_link"/>
        <origin xyz="0 0 30.17" rpy="0 0 0"/>
<!--        <axis xyz="1 0 0"/>-->
<!--        <limit lower="0" upper="3.14"/>-->
    </joint>
</robot>